{
  "name": "mobile-pdf-editor",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__.*\\.test\\.(ts|tsx)$ --testPathIgnorePatterns=e2e|performance|accessibility|cross-platform",
    "test:integration": "jest --testPathPattern=integration\\.test\\.(ts|tsx)$",
    "test:e2e": "jest --testPathPattern=e2e.*\\.test\\.(ts|tsx)$ --runInBand",
    "test:performance": "jest --testPathPattern=performance.*\\.test\\.(ts|tsx)$ --runInBand",
    "test:accessibility": "jest --testPathPattern=accessibility.*\\.test\\.(ts|tsx)$",
    "test:cross-platform": "jest --testPathPattern=cross-platform.*\\.test\\.(ts|tsx)$ --runInBand",
    "test:comprehensive": "ts-node src/__tests__/run-comprehensive-tests.ts",
    "test:pipeline": "ts-node src/__tests__/pipeline/test-pipeline.config.ts",
    "test:ci": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:all": "npm run test:comprehensive",
    "test:final": "jest --testPathPatterns=\"final-integration\\.test\\.(ts|tsx)$\"",
    "prepare-deployment": "node ./scripts/prepare-deployment.js",
    "build:ios": "expo build:ios",
    "build:android": "expo build:android",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.12",
    "expo-dev-client": "^6.0.20",
    "expo-document-picker": "^14.0.8",
    "expo-file-system": "^19.0.21",
    "expo-font": "~14.0.10",
    "expo-haptics": "~15.0.8",
    "expo-image": "~3.0.11",
    "expo-linking": "~8.0.11",
    "expo-media-library": "^18.0.5",
    "expo-router": "~6.0.23",
    "expo-sharing": "^14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "expo-system-ui": "~6.0.9",
    "expo-web-browser": "~15.0.10",
    "pdf-lib": "^1.17.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.0",
    "babel-jest": "^30.2.0",
    "babel-preset-expo": "^54.0.9",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "react-test-renderer": "^19.1.0",
    "ts-jest": "^29.4.6",
    "typescript": "~5.9.2"
  },
  "private": true
}
